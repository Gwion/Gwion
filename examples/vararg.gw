fun void test(...) {
  int i;
  vararg.start;

	if(i == 0)
		<<< vararg.i >>>;
	else if(i == 1)
		<<< vararg.f >>>;
	else if(i == 2)
		<<< vararg.c >>>;
	else if(i == 3)
		<<< vararg.p >>>;
	else if(i == 4)
		<<< vararg.v3 >>>;
	else if(i == 5)
		<<< vararg.v4 >>>;
	else if(i == 6)
		<<< vararg.o >>>;
  	i++;
  vararg.end;
}
test(1);
test(1, 2.3, complex(3, 2), polar(3, 2.1), Vec3(1,2,3), Vec4(1,2,3,4), null);
