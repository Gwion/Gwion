#! define a fun pointer type
typedef void Test();

#! define a few funs
fun void test1(){ <<< "test" >>>; };
fun void test2(){ <<< "another test" >>>; };
fun void test3(){ <<< "yet another test" >>>; };

#! create a fun pointer instance
var Test test;

#! assign it a fun
test1 @=> test;
#! test it
test();

#!  ...
test2 @=> test;
test();

test3 @=> test;
test();

<<<  test3 $ Test >>>;
