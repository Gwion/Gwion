class C
{
	var int i;
	var float f;
	var Object o;

	fun int     m_i() { return i; }
	fun float   m_f() { return f; }
	fun Object  m_o() { return o; }
}

var C c;
#! read members
<<<  c.i, c.f, c.o  >>>;

#! call fun members
<<<  c.m_i(), " ", c.m_f(), " ", c.m_o() >>>;

#! write members
<<<  12  => c.i  >>>;
<<<  1.2 => c.f  >>>;
